# Node Environment
NODE_ENV=development

# Server Configuration
PORT=3000
HOST=localhost

# Cluster Configuration
CLUSTER_NODES=localhost:3000,localhost:3001,localhost:3002
NODE_ID=node0

# HNSW Configuration
HNSW_DIMENSION=128
HNSW_MAX_ELEMENTS=1000000
HNSW_M=16
HNSW_EF_CONSTRUCTION=200
HNSW_EF_SEARCH=50
HNSW_ML=1.0

# Raft Configuration
RAFT_HEARTBEAT_TIMEOUT=50
RAFT_ELECTION_TIMEOUT_MIN=150
RAFT_ELECTION_TIMEOUT_MAX=300
RAFT_BATCH_SIZE=100

# Monitoring Configuration
METRICS_INTERVAL=1000
HEALTH_CHECK_INTERVAL=500

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=pretty # or json

# WebSocket Configuration
WS_RECONNECT_INTERVAL=1000
WS_MAX_RECONNECT_ATTEMPTS=5
